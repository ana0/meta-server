{
  "name": "meta-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run server:watch",
    "build": "babel ./src --out-dir ./build --copy-files",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:new-migration": "./node_modules/.bin/sequelize-cli migration:generate --name",
    "db:migrate-down": "./node_modules/.bin/sequelize-cli db:migrate:undo",
    "server:build": "babel ./src/ --out-dir ./build/",
    "server:start": "cross-env NODE_ENV=production node ./build/index.js",
    "server:watch": "nodemon --watch ./src/ --exec cross-env NODE_ENV=development babel-node -r dotenv/config ./src/index.js --source-maps inline",
    "backend:start": "concurrently --prefix '{pid}-{name}' --names 'API,WORKERS' --kill-others 'npm run server:start' 'npm run worker:start'",
    "backend:watch": "concurrently --prefix '{pid}-{name}' --names 'API,WORKERS' --kill-others 'npm run server:watch' 'npm run worker:watch'",
    "worker:serve": "NODE_ENV=development babel-node -r dotenv/config ./src/worker.js --source-maps inline",
    "worker:start": "cross-env NODE_ENV=production WORKER=true node ./build/worker.js",
    "worker:watch": "WORKER=true nodemon --watch ./src --exec npm run worker:serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ana0/meta-server.git"
  },
  "author": "ana0",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ana0/meta-server/issues"
  },
  "homepage": "https://github.com/ana0/meta-server#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.15",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.14.5",
    "base64json": "^1.0.3",
    "body-parser": "^1.19.0",
    "bull": "^3.22.8",
    "celebrate": "^14.0.0",
    "compression": "^1.7.4",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^8.2.0",
    "ethers": "^6.6.4",
    "express": "^4.17.1",
    "good-death-contracts": "^1.0.4",
    "helmet": "^4.4.1",
    "http-status": "^1.5.0",
    "lifeforms-contracts": "^1.0.2",
    "method-override": "^3.0.0",
    "mime": "^3.0.0",
    "mirror-piece-contracts": "^1.0.3",
    "multer": "^1.4.5-lts.1",
    "off-contracts": "^1.0.9",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "svg64": "^2.0.0",
    "web3": "^1.3.5",
    "wildcards-contracts": "^1.0.3"
  }
}
